<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.forms">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>


    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_forms_document_svgrepo_com_cyan"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_forms_document_svgrepo_com_cyan"
        android:supportsRtl="true"
        android:theme="@style/Theme.Forms.cyan"
        android:requestLegacyExternalStorage="true">
        <activity android:name="com.app.forms.activities.CreateFormActivity"
            android:windowSoftInputMode="adjustPan"></activity>
        <activity
            android:name="com.app.forms.activities.MainActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>